SequenceDiagram {
 lifeline browser
 lifeline server
 lifeline seguridad
 lifeline controlador
 lifeline modelo

  browser --> server 'post/usuarios'
  server --> seguridad 'post/dameUsuarios(req.body)'
  seguridad --> controlador 'dameUsuarios(data)'
  controlador --> modelo 'getUsuarios()'
  controlador <-r- modelo '[{Usuario}]'
  seguridad <-r- controlador '[{Usuario}]'
  server <-r- seguridad '[{Usuario}]'
  browser <-r- server 'usuarios.ejs ( [{Usuario}] )'

}